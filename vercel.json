{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/node",
        "config": { "distDir": "dist" }
      },
      {
        "src": "api/**/*.js",
        "use": "@vercel/node"
      },
      {
        "src": "api/**/*.mjs",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/api/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/public/$1",
        "continue": true
      }
    ],
    "cleanUrls": true
  }
  