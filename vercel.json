{
  "version": 2,
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/node",
      "config": { "distDir": "dist" }
    },
    {
      "src": "api/server.mjs",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/server.mjs"
    },
    {
      "src": "/(.*)",
      "dest": "/src/$1"
    }
  ]
}